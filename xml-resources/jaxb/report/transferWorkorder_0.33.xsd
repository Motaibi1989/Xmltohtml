		<xs:sequence>
			<xs:element name="aircraft" type="xs:string">
				<xs:annotation>
					<xs:appinfo>AMOS_MIMETYPE=E_AC</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="position" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:group>
	<xs:group name="stationStoreLocationType">
