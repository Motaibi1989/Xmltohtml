<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sas="http://www.swiss-as.com/XML" elementFormDefault="unqualified" attributeFormDefault="unqualified" version="0.3">
	<!-- COMMON TYPES -->
	<!-- SIMPLE ISO8601 DATE -->
	<xs:simpleType name="simpleDateType">
		<xs:annotation>
			<xs:documentation>Date type. ISO8601-date as string or empty string.</xs:documentation>
			<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([1-9][0-9][0-9][0-9]\-((0[1-9])|(1[0-2]))\-((0[1-9])|([12][0-9])|([3][01])))?"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- SIMPLE UTC TIME -->
	<xs:simpleType name="simpleTimeUtcType">
		<xs:annotation>
			<xs:documentation>Time type. UTC-time as string or empty string.</xs:documentation>
			<xs:appinfo>AMOS_MIMETYPE=D_TIME</xs:appinfo>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="(([01][0-9][:][0-5][0-9])|(2[0-3][:][0-5][0-9]))?"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- YES OR NO -->
	<xs:simpleType name="yesNoType">
		<xs:annotation>
			<xs:documentation>Y=YES; N=NO.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Y">
				<xs:annotation>
					<xs:documentation>YES.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="N">
				<xs:annotation>
					<xs:documentation>NO.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- PRIMITIVE TYPES -->
	<xs:simpleType name="positiveInteger">
		<xs:annotation>
			<xs:documentation>Zero or positive integer</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="2147483647"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="integer">
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="-2147483648"/>
			<xs:maxInclusive value="2147483647"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- ATA CHAPTER -->
	<xs:simpleType name="ataChapter">
		<xs:restriction base="xs:string">
			<xs:maxLength value="12"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- STATION -->
	<xs:simpleType name="station">
		<xs:annotation>
			<xs:documentation>Max. 4 characters</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- USER SIGN -->
	<xs:simpleType name="userSignSimpleType">
		<xs:annotation>
			<xs:documentation>Any user sign. Max. 8 characters.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- PARTNUMBER DEFINITION -->
	<xs:simpleType name="partNumberType">
		<xs:annotation>
			<xs:documentation>Type for partnumbers: minLentgh=1; maxLength=20;</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!--PARTSERIALNUMBER DEFINITION -->
	<xs:simpleType name="partSerialNumberType">
		<xs:annotation>
			<xs:documentation>Type for part-serialnumbers: minLength=1; maxLength=20</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<!-- COMMON TYPES finished -->
	<!-- PART/SERIAL NUMBER COMPLEX TYPE -->
	<xs:complexType name="partSerialNumberGroup">
		<xs:annotation>
			<xs:documentation>Groups a part and and a serial number</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="partNumber" type="partNumberType">
				<xs:annotation>
					<xs:documentation>part number. This field only supports up to 20 characters. Please use the latest schema version to accommodate part numbers up to 32 characters. </xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_part_on_off</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT=32</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="serialNumber" type="partSerialNumberType">
				<xs:annotation>
					<xs:documentation>serial number</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_part_on_off</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- WO_TYPE -->
	<xs:simpleType name="workorderTypeType">
		<xs:annotation>
			<xs:documentation>Type of the workorder: P = Pirep, C = Cabin, M = Maintenance, S = Scheduled
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="P">
				<xs:annotation>
					<xs:documentation>Pirep.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Cabin.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="M">
				<xs:annotation>
					<xs:documentation>Maintenance.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="S">
				<xs:annotation>
					<xs:documentation>Scheduled.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- WO_STATE -->
	<xs:simpleType name="workorderStateType">
		<xs:annotation>
			<xs:documentation>State of the workorder: O = open Workorder,  C = closed Workorder
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="O">
				<xs:annotation>
					<xs:documentation>Open workorder.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="C">
				<xs:annotation>
					<xs:documentation>Closed workorder.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- GROUPS -->
