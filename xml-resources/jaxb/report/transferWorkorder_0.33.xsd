				<xs:annotation>
					<xs:appinfo>AMOS_MIMETYPE=A_STATION</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="store" type="xs:string" minOccurs="0"/>
			<xs:element name="location" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:group>-->
	<!-- WORKORDER -->
	<xs:complexType name="workorderType">
		<xs:all>
			<xs:element name="workorderNumber" type="positiveInteger">
				<xs:annotation>
					<xs:documentation>The documents workorder-number.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=E_WO</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=wo_header.workorderno</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="orgWorkorderNumber" type="positiveInteger" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Original workorder-number after a change.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=E_WO</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="deleted" type="yesNoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Deleted Y or N. No header is required if deleted.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="workorderHeader" type="workorderHeaderType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Further information regarding the workorder.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- REQUIRED_PARTSCollection -->
	<xs:complexType name="requiredPartsCollection">
		<xs:sequence>
			<xs:element name="requiredPart" type="requiredPartType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- REQUIRED_PARTType-->
	<xs:complexType name="requiredPartType">
		<xs:all minOccurs="0">
			<xs:element name="partNumber" type="partNumberType">
				<xs:annotation>
					<xs:documentation>The required parts number. This field only supports up to 20 characters. Please use the latest schema version to accommodate part numbers up to 32 characters. </xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=M_PART</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=part_request.partno</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT=32</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="partDescription" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Closer description to the partnumber.(Only used for EXPORT.)</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part.description</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="quantity" type="positiveInteger">
				<xs:annotation>
					<xs:documentation>Required number of parts.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_request.qty</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="unitOfMeasurement" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unit of measurement of the required part. Only filled on export. Ignored on import.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part.measure_unit</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="isTool" type="yesNoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indication that the required part is a tool. Only filled on export. Ignored on import.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part.tool</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="externallyProvisioned" type="yesNoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Flag indication that the required part is externally provisioned. Only filled on export. Ignored on import.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_request_2 .externally_provisioned</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="ipcReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>IPC Reference.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=part_request.ipc_reference</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="userSign" type="userSignSimpleType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>User sign. Not filled with part request framework</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- ROTABLEType-->
	<xs:complexType name="rotableType">
		<xs:all minOccurs="0">
			<xs:element name="partNumber" type="partNumberType">
				<xs:annotation>
					<xs:documentation>The rotables part number. This field only supports up to 20 characters. Please use the latest schema version to accommodate part numbers up to 32 characters. </xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=M_PART</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=wo_header.partno</xs:appinfo>
					<xs:appinfo>AMOS_CHECKED_LENGTH_EXPORT=32</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="serialNumber" type="partSerialNumberType">
				<xs:annotation>
					<xs:documentation>The rotables serial number.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_header.serialno</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="position" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The rotables position.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_header.ac_position</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="14"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- WO_REFERENCESCollection -->
	<xs:complexType name="workorderReferencesCollection">
		<xs:sequence>
			<xs:element name="reference" type="workorderReferenceType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- WO_REFERENCEType -->
	<xs:complexType name="workorderReferenceType">
		<xs:all minOccurs="0">
			<xs:element name="referenceNumber">
				<xs:annotation>
					<xs:documentation>Number of the referenced document.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=db_link.description</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="255"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="referenceType">
				<xs:annotation>
					<xs:documentation>Type of reference. I.E. T=Taskcard, W=Workorder, D=Document, F=Flight-Log. Custom reference types allowed but max. 5 characters
					</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=db_link.ref_type</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="8"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="remarks" minOccurs="0">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="36"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<!-- TRANSFERCollection -->
	<xs:complexType name="transferCollection">
		<xs:sequence>
			<xs:element name="transfer" type="transferType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<!-- TRANSFERType-->
	<xs:complexType name="transferType">
		<xs:all minOccurs="0">
			<xs:element name="transferIndex" type="integer">
				<xs:annotation>
					<xs:documentation>Index of the transfer. ONLY THE TRANSFER WITH THE HIGHEST INDEX COUNTS.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.recno</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="workpackageNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Workpackage number. Only filled if a workorder is assigned to a workpackage.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wp_header.wpno</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="plannedDate" type="simpleDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Planned date of the work. Only filled if a workorder is assigned to a workpackage.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.planned_date</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="expectedDate" type="simpleDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Expected Date. Depending on the due-values and the flight-log. Information only, will not be imported.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=forecast.expected_date</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="priority" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The workorders priority.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.prio</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="checkType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Filled if you transfer a workorder to a check type. (Not on every layout possible.)</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.check_type</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="transferDate" type="simpleDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Date at transfer.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.date_transfer</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="transferTime" type="simpleTimeUtcType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Time at transfer.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.transfer_time</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="transferStation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Station at transfer.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.transfer_station</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="dueDate" type="simpleDateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Due date.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_DATE</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.transfer_date</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueTah" type="integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>TAH due.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.due_tah</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="dueTac" type="integer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>TAC due.</xs:documentation>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.due_tac</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="noLimit" type="yesNoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>No limit set: YES or NO.</xs:documentation>
					<xs:appinfo>AMOS_MIMETYPE=D_BOOLEAN</xs:appinfo>
					<xs:appinfo>AMOS_DATABASE=wo_transfer.no_limit</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="mechSign" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Mechanic sign. User or mech sign is mandatory</xs:documentation>
